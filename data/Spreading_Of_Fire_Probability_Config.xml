<?xml version="1.0"?>
<CellAutomata>
  <NumberOfRows>10</NumberOfRows>
  <NumberOfColumns>10</NumberOfColumns>
  <SimulationType>3</SimulationType>
  <Title>Spreading of Fire</Title>
  <Author>Edison Ooi, Matt Knox, Haris Adnan</Author>
  <Description>Simulation of Spreading of Fire with each cell initialized randomly</Description>
  <NumberOfNeighborRows>3</NumberOfNeighborRows>
  <NumberOfNeighborColumns>3</NumberOfNeighborColumns>
  <NeighborConfig>
    1 1 1
    1 X 1
    1 1 1
  </NeighborConfig>
  <StartingConfig>
    Probability 0.9 0.05 0.05
  </StartingConfig>
  <probCatch>1</probCatch>
  <probGeneration>0.5</probGeneration>
</CellAutomata>